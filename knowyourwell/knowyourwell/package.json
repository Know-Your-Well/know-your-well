{
  "name": "knowyourwell",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"cd ClientApp && npm run start\" \"node index.js\"",
    "build": "cd ClientApp && npm install && npm run build && cd .. && npm install",
    "server": "nodemon index.js",
    "client": "cd ClientApp && npm start && cd ..",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "test": "jest",
    "pretty": "prettier --write \"**/*.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@authenio/samlify-node-xmllint": "^2.0.0",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "mssql": "^9.1.1",
    "reactstrap": "^9.2.2",
    "samlify": "^2.8.11",
    "semantic-ui-react": "^2.1.5",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/@azure/storage-blob(?!(lib))"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|jpeg|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "^react($|/.+)": "<rootDir>/node_modules/react$1",
      "^react-router-dom($|/.+)": "<rootDir>/node_modules/react-router-dom$1"
    }
  }
}
