    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "mssql": "^9.1.1",
    "mysql": "^2.18.1",
    "react-bootstrap": "^2.9.1",
    "react-datetime": "^3.2.0",
    "react-detect-offline": "^2.4.5",
